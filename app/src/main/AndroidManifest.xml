<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Sistema Estudiantes"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prueba2Android"
        tools:targetApi="31">
        <activity
            android:name=".VerActivity"
            android:exported="false" />
        <activity
            android:name=".ListarActivity"
            android:exported="false" /> <!-- Splash Screen - LAUNCHER -->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- Agregar Estudiante -->
        <activity
            android:name=".AgregarActivity"
            android:exported="false" /> <!-- Editar Estudiante -->
        <activity
            android:name=".EditarActivity"
            android:exported="false" />
    </application>

</manifest>